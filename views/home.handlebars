<h1>Ol√° sequelize!</h1>
<div class="container">
 {{#each users}}
   <div class="item">
      <h2>{{this.name}}</h2>
      <p>{{this.occupation}}</p>
  <div class="container-bottoms">
      <div class="actions">
          <a href="/users/{{this.id}}" class="details-btn">Ver detalhes</a>
          <a href="/users/edit/{{this.id}}" class="details-btn">Editar</a>
      </div>
      
      <div class="actions">
        <form action="/users/delete/{{this.id}}" method="POST">
        <input type="submit" value="Excluir" class="delete-btn">
        </form>
      </div>
   </div>
 </div>
 {{/each}}
</div>